{
    "version": "0.2.0",
    "configurations": [
		{
			"name": "Launch localhost",
			"type": "firefox",
			"request": "launch",
			"reAttach": true,
			"url": "http://localhost:3000/",
			"webRoot": "${workspaceFolder}",
			"pathMappings": [
				{
					"url": "http://localhost:3000/src/Player.js",
					"path": "${workspaceFolder}/src/Player.js"
				}
			]
		}
	]
}